{
  "name": "@tldraw/svg",
  "version": "0.1.3",
  "description": "SVG utilities for TLDraw and maybe you, too.",
  "author": "@steveruizok",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tldraw/svg.git"
  },
  "license": "MIT",
  "private": false,
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "2d",
    "vector",
    "typescript",
    "javascript"
  ],
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/index.d.ts",
  "targets": {
    "default": {
      "distDir": "./dist",
      "isLibrary": true
    }
  },
  "sourceMap": "inline",
  "scripts": {
    "start": "parcel watch",
    "build": "parcel build && tsc --project tsconfig.build.json",
    "check": "tsc --noEmit",
    "lint": "eslint"
  },
  "dependencies": {},
  "devDependencies": {
    "@parcel/packager-ts": "^2.0.0",
    "@parcel/transformer-js": "^2.0.0",
    "@parcel/transformer-typescript-types": "^2.0.0",
    "parcel": "latest",
    "tslib": "^2.3.1",
    "typescript": ">=3.0.0"
  },
  "peerDependencies": {}
}